<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.scoula.mypage.favorite.mapper.ProductMapper">

    <select id="getAllSubcategories" resultType="org.scoula.mypage.favorite.dto.SubcategoryResponse">
        SELECT subcategory_id AS subcategoryId, name AS subcategoryName
        FROM product_subcategory
    </select>

</mapper>
